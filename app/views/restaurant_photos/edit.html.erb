<div class="container">
  <ol class="breadcrumb">
   <li class="breadcrumb-item"> My Eatery </li>
   <li class="breadcrumb-item"> <%= @restaurant.name %></li>
   <li class="breadcrumb-item active"> <%=link_to 'Photos', restaurant_restaurant_photos_path(@restaurant) %></li>
  </ol>
  <div class="row col-md-6 col-md-offset-3">
    <h6 class="text-center"> Edit Photo </h6>
    <%=form_for [@restaurant, @restaurant_photo] do |f| %>
      <%= render partial: 'form', locals: {f: f} %>
    <% end %>
  </div>
</div>

